		end
	end
	spawn(function()
		game:GetService("RunService").RenderStepped:Connect(function()
			if _G.AutoFram then
				pcall(function()
					CheckLevel()
					if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false then
						Tween(CFrameQuest)
						if (CFrameQuest.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < 50 then
							wait(0.3)
							local args = {
								[1] = "StartQuest",
								[2] = QuestName,
								[3] = QuestNumber
							}
							game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
						end
					end
				end)
			end
		end)
	end)
	spawn(function()
		game:GetService("RunService").RenderStepped:Connect(function()
			if _G.AutoFram then
				if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == true then
					CheckLevel()
					pcall(function()
						for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
							for x,y in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
								if v.Name == Mon and y.Name == Mon then
									v.HumanoidRootPart.Size = Vector3.new(60,2.5,60)
									v.HumanoidRootPart.CanCollide = false
									v.HumanoidRootPart.CFrame = y.HumanoidRootPart.CFrame
									v.Humanoid.WalkSpeed = 0
									v.Humanoid.JumpPower = 0
									sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
									Tween(v.HumanoidRootPart.CFrame * CFrame.new(0,0,25))
								end
							end
						end
					end)
				end
			end
		end)
	end)
	spawn(function()
		game:GetService("RunService").RenderStepped:Connect(function()
			if _G.AutoFram then
				if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == true then
					CheckLevel()
					pcall(function()
						if not game:GetService("Workspace").Enemies:FindFirstChild(Mon) then
							Tween(CFramePuk)
						end
					end)
				end
			end
		end)
	end)
	spawn(function()
		game:GetService("RunService").RenderStepped:Connect(function()
			if _G.AutoFram then
				CheckLevel()
				pcall(function()
					for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
						if v.Name == Mon then
							if v.Humanoid.Health == 0 then
								_G.Attack = false
								wait(0.1)
								v:Destroy()
							elseif v.Humanoid.Health >= 1 then
								if (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 50 then
									_G.Attack = true
								else
									_G.Attack = false
								end
							end
						end
					end
				end)
			end
		end)
	end)
	spawn(function()
		game:GetService("RunService").RenderStepped:Connect(function()
			if _G.Attack == true and _G.AutoFram == true then
				game:GetService'VirtualUser':CaptureController()
				game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672))
			end
		end)
	end)
	spawn(function()
		game:GetService("RunService").RenderStepped:Connect(function()
			pcall(function()
				if _G.AutoFram then
					CheckLevel()
					if not string.find(game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, Mon) then
						game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack("AbandonQuest"))
					end
				end
			end)
		end)
	end)
	
end
coroutine.wrap(XYBFEM_fake_script)()
local function BQWWB_fake_script() -- AutoEquip.ep 
	local script = Instance.new('LocalScript', AutoEquip)

	local a = script.Parent.Button
	local b = script.Parent.Text
	local Names = game.Players.LocalPlayer.Name..game.PlaceId
	loadstring(game:HttpGet('https://raw.githubusercontent.com/b3styskid/MAZI-API/main/SaveSettingAPI.lua'))()
	getgenv().Setting = {
		["AutoEquip"] = false
	}
	Get_Setting(Names)
	pcall(function()
		getgenv().Setting.AutoEquip = Get_Setting(Names).AutoEquip
		if Get_Setting(Names).AutoEquip == true then
			b.BackgroundColor3 = Color3.new(0, 1, 1)
		end
	end)
	a.MouseButton1Click:Connect(function()
		if Get_Setting(Names).AutoEquip == true then
			getgenv().Setting.AutoEquip = false
			b.BackgroundColor3 = Color3.new(0.137255, 0.137255, 0.137255)
		else
			getgenv().Setting.AutoEquip = true
			b.BackgroundColor3 = Color3.new(0, 1, 1)
		end
		Update_Setting(Names)
	end)
	game:GetService("RunService").RenderStepped:Connect(function()
		if getgenv().Setting.AutoEquip == true then
			if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Combat") then
				game.Players.LocalPlayer.Character.Humanoid:EquipTool(game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Combat"))
			end
		end
	end)
end
coroutine.wrap(BQWWB_fake_script)()
local function VDDKU_fake_script() -- AutoBusoHaki.buso 
	local script = Instance.new('LocalScript', AutoBusoHaki)

	local a = script.Parent.Button
	local b = script.Parent.Text
	local Names = game.Players.LocalPlayer.Name..game.PlaceId
	_G.AutoHaki = false
	loadstring(game:HttpGet('https://raw.githubusercontent.com/b3styskid/MAZI-API/main/SaveSettingAPI.lua'))()
	getgenv().Setting = {
		["Autobuso"] = false
	}
	Get_Setting(Names)
	pcall(function()
		getgenv().Setting.Autobuso = Get_Setting(Names).Autobuso
		if Get_Setting(Names).Autobuso == true then
			b.BackgroundColor3 = Color3.new(0, 1, 1)
			_G.AutoHaki = true
		end
	end)
	a.MouseButton1Click:Connect(function()
		if Get_Setting(Names).Autobuso == true then
			getgenv().Setting.Autobuso = false
			b.BackgroundColor3 = Color3.new(0.137255, 0.137255, 0.137255)
			_G.AutoHaki = false
		else
			getgenv().Setting.Autobuso = true
			b.BackgroundColor3 = Color3.new(0, 1, 1)
			_G.AutoHaki = true
		end
		Update_Setting(Names)
	end)
	game:GetService("RunService").RenderStepped:Connect(function()
		if _G.AutoHaki then
			pcall(function()
				if not game.Players.LocalPlayer.Character.Humanoid:FindFirstChild("LeftHand_BusoLayer1") then
					game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
				end
			end)
		end
	end)
end
coroutine.wrap(VDDKU_fake_script)()
local function VCFIG_fake_script() -- AutoKen.ken 
	local script = Instance.new('LocalScript', AutoKen)

	local a = script.Parent.Button
	local b = script.Parent.Text
	local Names = game.Players.LocalPlayer.Name..game.PlaceId
	loadstring(game:HttpGet('https://raw.githubusercontent.com/b3styskid/MAZI-API/main/SaveSettingAPI.lua'))()
	getgenv().Setting = {
		["AutoKen"] = false
	}
	Get_Setting(Names)
	pcall(function()
		getgenv().Setting.AutoKen = Get_Setting(Names).AutoKen
		if Get_Setting(Names).AutoKen == true then
			b.BackgroundColor3 = Color3.new(0, 1, 1)
		end
	end)
	a.MouseButton1Click:Connect(function()
		if Get_Setting(Names).AutoKen == true then
				getgenv().Setting.AutoKen = false
				b.BackgroundColor3 = Color3.new(0.137255, 0.137255, 0.137255)
			else
				getgenv().Setting.AutoKen = true
				b.BackgroundColor3 = Color3.new(0, 1, 1)
			end
		Update_Setting(Names)
	end)
	game:GetService("RunService").RenderStepped:Connect(function()
		if getgenv().Setting.AutoKen == true then
			local args = {
				[1] = "Ken",
				[2] = true
			}
			game:GetService("ReplicatedStorage").Remotes.CommE:FireServer(unpack(args))
		end
	end)
end
coroutine.wrap(VCFIG_fake_script)()
