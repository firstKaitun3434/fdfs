-- Scripts:

local function XSCMC_fake_script() -- BothubFrame.LocalScript 
	local script = Instance.new('LocalScript', BothubFrame)

	local part = script.Parent.tween
	local part2 = script.Parent.Parent.BothubFrame
	local mouse = game:GetService("UserInputService")
	local b
	game:GetService("RunService").RenderStepped:Connect(function()
		part.MouseButton1Down:Connect(function()
			b = (part2.Position - UDim2.new(0, mouse:GetMouseLocation().X - 600 , 0, mouse:GetMouseLocation().Y - 300))
			_G.a = true
		end)
		part.MouseButton1Up:Connect(function()
			_G.a = false
		end)
	end)
	game:GetService("RunService").RenderStepped:Connect(function()
		if _G.a then
			part2.Position = b + UDim2.new(0, mouse:GetMouseLocation().X - 600 , 0, mouse:GetMouseLocation().Y - 300)
		end
	end)
	
end
coroutine.wrap(XSCMC_fake_script)()
local function XYBFEM_fake_script() -- AutoFramLevel.Level 
	local script = Instance.new('LocalScript', AutoFramLevel)

	local a = script.Parent.Button
	local b = script.Parent.Text
	local Names = game.Players.LocalPlayer.Name..game.PlaceId
	loadstring(game:HttpGet('https://raw.githubusercontent.com/b3styskid/MAZI-API/main/SaveSettingAPI.lua'))()
	getgenv().Setting = {
		["AutoFram"] = false
	}
	Get_Setting(Names)
	pcall(function()
		getgenv().Setting.AutoFram = Get_Setting(Names).AutoFram
		if Get_Setting(Names).AutoFram == true then
			b.BackgroundColor3 = Color3.new(0, 1, 1)
			_G.AutoFram = true
		end
	end)
	a.MouseButton1Click:Connect(function()
		if Get_Setting(Names).AutoFram == true then
			getgenv().Setting.AutoFram = false
			b.BackgroundColor3 = Color3.new(0.137255, 0.137255, 0.137255)
			_G.AutoFram = false
		else
			getgenv().Setting.AutoFram = true
			b.BackgroundColor3 = Color3.new(0, 1, 1)
			_G.AutoFram = true
		end
		Update_Setting(Names)
	end)
	function Tween(P)
		local Pos = (P.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
		local nService = game:GetService("TweenService")
		local info = TweenInfo.new(Pos/350, Enum.EasingStyle.Linear)
		local Warp = nService:Create(game:GetService("Players").LocalPlayer.Character["HumanoidRootPart"], info, {CFrame = P})
		Warp:Play()
		if Pos <= 50 then
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = P
		end
	end
	function CheckLevel()
		local Level = game:GetService("Players").LocalPlayer.Data.Level.Value
		if Level <= 9 then
			Mon = "Bandit"
			SpawnPoint = "Default"
			QuestName = "BanditQuest1"
			QuestNumber = 1
			CFrameQuest = CFrame.new(1059.9759521484375, 16.488065719604492, 1549.023193359375)
			CFramePuk = CFrame.new(1108.1478271484375, 57.41001510620117, 1623.7769775390625)
		elseif Level <= 14 then
			Mon = "Monkey"
			SpawnPoint = "Jungle"
			QuestName = "JungleQuest"
			QuestNumber = 1
			CFrameQuest = CFrame.new(-1599.5264892578125, 36.977455139160156, 153.2390899658203)
			CFramePuk = CFrame.new(-1586.5660400390625, 39.97743606567383, 155.67477416992188)
		elseif Level == 15 or Level <= 29 then
			Mon = "Gorilla"
			SpawnPoint = "Jungle"
			QuestName = "JungleQuest"
			QuestNumber = 2
